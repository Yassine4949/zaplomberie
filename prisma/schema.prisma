generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Booking {
  id            String   @id @default(cuid())
  name          String
  phone         String
  email         String?
  address       String?
  serviceType   String?
  message       String?
  preferredDate DateTime?
  status        String   @default("NEW")
  createdAt     DateTime @default(now())
}
